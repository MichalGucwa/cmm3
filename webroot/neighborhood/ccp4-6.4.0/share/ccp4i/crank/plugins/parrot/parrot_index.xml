<?xml version="1.0" standalone="yes"?>

<crank>
   <plugin id="parrot">
      <name>parrot</name>
      <executable>cparrot</executable>
      <tclproc>parrot_run</tclproc>
      <tclfile>crank_parrot.tcl</tclfile>
      <cccp4file>cccp4_parrot.tcl</cccp4file>
      <cccp4setupproc>parrot_setup</cccp4setupproc>
      <cccp4deffile>cccp4_parrot.def</cccp4deffile>
      <nextprogram>buccaneer</nextprogram>
      <input>
         <substructure>1</substructure>
	 <experimental_columns>1</experimental_columns>
         <hl_columns>1</hl_columns>
         <freer_columns>1</freer_columns>
      </input>
      <output>
         <phase_columns>1</phase_columns>
         <hl_columns>1</hl_columns>
      </output>
   </plugin>
</crank>

