<?xml version="1.0" standalone="yes"?>

<crank>
   <plugin id="phaser">
      <name>phaser</name>
      <executable>phaser</executable>
      <tclproc>phaser_run</tclproc>
      <tclfile>crank_phaser.tcl</tclfile>
      <cccp4file>cccp4_phaser.tcl</cccp4file>
      <cccp4setupproc>phaser_setup</cccp4setupproc>
      <cccp4deffile>cccp4_phaser.def</cccp4deffile>
      <nextprogram>check</nextprogram>
      <input>
         <experimental_columns>1</experimental_columns>
         <pdb>1</pdb>
      </input>
      <output>
         <pdb>1</pdb>
         <experimental_columns>1</experimental_columns>
         <phase_columns>1</phase_columns>
         <hl_columns>1</hl_columns>
      </output>
   </plugin>
</crank>

