<?xml version="1.0" standalone="yes"?>

<crank>
   <plugin id="shelxc">
      <name>shelxc</name>
      <executable>shelxc</executable>
      <tclproc>shelxc_run</tclproc>
      <tclfile>crank_shelxc.tcl</tclfile>
      <cccp4file>cccp4_shelxc.tcl</cccp4file>
      <cccp4setupproc>shelxc_setup</cccp4setupproc>
      <cccp4deffile>cccp4_shelxc.def</cccp4deffile>
      <nextprogram>shelxd</nextprogram>
      <input>
         <experimental_columns>1</experimental_columns>
      </input>
      <output>
         <i_columns>1</i_columns>
         <f_columns>1</f_columns>
         <fa_columns>1</fa_columns>
         <alpha_columns>1</alpha_columns>
      </output>
   </plugin>
</crank>

